{
    "name": "脚本测试",
    "version": "0.0.3",
    "type": "script",
    "description": "这是一个测试脚本，只会进行打印",
    "schema": {
        "layouts": [
            {
                "key": "name",
                "alias": "测试名称",
                "type": "input",
                "placeholder": "请输入测试脚本流程名称",
                "rules": [
                    "Required"
                ]
            },
            {
                "key": "openDebugLogger",
                "alias": "测试开关",
                "type": "switch",
                "help": "打开将展示测试日志",
                "placeholder": "是否开启测试日志"
            }
        ],
        "dependencies": [],
        "script": ""
    }
}